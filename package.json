{
  "name": "chatta_react",
  "version": "0.0.1",
  "main": "dist",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:server": "nodemon server/index.js --exec babel-node --ignore 'client/*'",
    "start:client": "cd client && npm run start",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:db": "docker-compose up",
    "clean": "rimraf dist",
    "build": "npm run clean && babel server -d dist",
    "now-build": "NODE_ENV=production npm run clean && babel server -d dist",
    "now-start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint server",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/server/test/setup.js",
    "moduleDirectories": [
      "node_modules",
      "server"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-redis": "^3.3.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-force-ssl": "^0.3.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.9",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^2.1.1",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^5.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^4.0.1",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.5.0",
    "mongodb-memory-server": "^2.0.0",
    "nodemon": "^1.18.3",
    "now": "^11.4.2",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0"
  }
}
